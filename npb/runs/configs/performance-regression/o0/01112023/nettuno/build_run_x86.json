{
    "info": {},
    "tags": [
        "exec_time"
    ],
    "executables": [
        "bt_x86_64_aligned.out",
        "cg_x86_64_aligned.out",
        "ep_x86_64_aligned.out",
        "ft_x86_64_aligned.out",
        "is_x86_64_aligned.out",
        "lu_x86_64_aligned.out",
        "mg_x86_64_aligned.out",
        "sp_x86_64_aligned.out",
        "ua_x86_64_init.out"
    ],
    "env": {},

    "*": {
        "iterations": 3,
        "output": "run_${executable}_${npb_class}_${iteration}.log",
        "prepend": [],
        "append": [],
        "build": [
            {
                "before": true,
                "args": [
                    "make",
                    "clean"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "${npb_class}"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "-C",
                    "${build_dir}",
                    "-j10",
                    "${executable}",
                    "OPT_LEVEL=-O0",
                    "NO_STACKMAPS=1",
                    "EXPERIMENT_MODE=1",
                    "DEBUG_ASM=1",
                    "LLC_FLAGS=\"-disable-block-align --mc-relax-all -aarch64-csr-alignment=8 -align-bytes-to-four -reg-scavenging-slot -align-objects-to-four -enable-misched=false -pack-indexes\"",
                    "LLC_FLAGS_ARM64=\"-mattr=+disable-fp-imm-materialize,+avoid-wide-mul,+copy-zr-temp,-disable-hoist-in-lowering,-avoid-f128,+x86-fabs-fneg,+x86-select,+advanced-ra-split-cost,-mov-64-imm32\"",
                    "LLC_FLAGS_X86=\"-mattr=+simple-reg-offset-addr,+aarch64-legal-addr,-multiply-with-imm,+aarch64-sized-imm,-non-zero-imm-to-mem,+aarch64-constant-cost-model,+force-vector-mem-op,+avoid-opt-mul-1,+avoid-select-to-fsetcc,+aarch64-legal-addr -x86-enable-simplify-cfg -no-x86-call-frame-opt -disable-x86-frame-obj-order -enable-lea32 -mul-constant-optimization=false -disable-gr32temp-inflate -aarch64-fcmp-cost\""
                ],
                "output": "${build_dir}/${executable}_${npb_class}_build.log"
            },
            {
                "before": false,
                "args": [
                    "cp",
                    "${build_dir}/${executable}",
                    "${dest_dir}"
                ]
            },
            {
                "before": false,
                "args": [
                    "mv",
                    "${build_dir}/${executable}_${npb_class}_build.log",
                    "${dest_dir}"
                ]
            },
            {
                "before": false,
                "args": [
                    "size",
                    "-A",
                    "-d",
                    "${build_dir}/${executable}"
                ],
                "output": "${dest_dir}/${executable}_${npb_class}_size.log"
            }
        ],
        "run": [
            {
                "before": false,
                "args": [
                    "grep",
                    "-vzq",
                    "\"UNSUCCESSFUL\"",
                    "run_${executable}*.log"
                ],
                "output": ""
            },
            {
                "before": false,
                "args": [
                    "grep",
                    "-h",
                    "\"Time in\"",
                    "run_${executable}*.log"
                ],
                "output": "time_${executable}_${npb_class}.log"
            },
            {
                "before": false,
                "args": [
                    "rm",
                    "-rf",
                    "run_${executable}*.log"
                ],
                "output": ""
            }
        ],
        "post_process": [
            {
                "args": [
                    "grep",
                    "-o",
                    "'[0-9\\.][0-9\\.]*'",
                    "time_${executable}_${npb_class}.log"
                ],
                "output": "data.log"
            },
            {
                "args": [
                    "LC_NUMERIC=\"C\"",
                    "awk",
                    "'{sum+=sprintf(\"%f\",$1)}END{printf \"%.2f\\n\",sum/NR}'",
                    "data.log"
                ],
                "output": "avg.log"
            },
            {
                "args": [
                    "echo -n",
                    "${benchmark},",
                    ">>results_${npb_class}.csv"
                ]
            },
            {
                "args": [
                    "cat",
                    "avg.log",
                    ">>results_${npb_class}.csv"
                ]
            },
            {
                "args": [
                    "rm",
                    "data.log",
                    "avg.log"
                ]
            }
        ]
    }
}
