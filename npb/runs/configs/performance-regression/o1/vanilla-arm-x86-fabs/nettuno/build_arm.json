{
    "info": {},
    "tags": [
        "exec_time"
    ],
    "executables": [
        "lu_aarch64_init.out",
        "sp_aarch64_init.out",
        "ua_aarch64_init.out"
    ],
    "env": {},

    "*": {
        "npb_class": "A",
        "output": "run_${executable}_${npb_class}_${iteration}.log",
        "prepend": [],
        "append": [],
        "build": [
            {
                "before": true,
                "args": [
                    "make",
                    "clean"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "${npb_class}"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "-C",
                    "${build_dir}",
                    "-j10",
                    "${executable}",
                    "OPT_LEVEL=-O1",
                    "UNMODIFIED=1"
                ],
                "output": "${build_dir}/${executable}_${npb_class}_build.log"
            },
            {
                "before": false,
                "args": [
                    "cp",
                    "${build_dir}/${executable}",
                    "${dest_dir}"
                ]
            },
            {
                "before": false,
                "args": [
                    "mv",
                    "${build_dir}/${executable}_${npb_class}_build.log",
                    "${dest_dir}"
                ]
            },
            {
                "before": false,
                "args": [
                    "size",
                    "-A",
                    "-d",
                    "${build_dir}/${executable}"
                ],
                "output": "${dest_dir}/${executable}_${npb_class}_size.log"
            }
        ],
        "run": []
    }
}
