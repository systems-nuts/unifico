{
    "info": {},
    "tags": [
        "exec_time"
    ],
    "executables": [
        "ep_x86_64_aligned.out",
        "is_x86_64_aligned.out"
    ],
    "env": {},

    "*": {
        "npb_class": "S",
        "iterations": 3,
        "output": "run_${executable}_${iteration}.log",
        "prepend": [],
        "append": [],
        "build": [
            {
                "before": true,
                "args": [
                    "make",
                    "clean"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "${npb_class}"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "-C",
                    "${build_dir}",
                    "-j10",
                    "${executable}"
                ],
                "output": "${build_dir}/${executable}_build.log"
            },
            {
                "before": false,
                "args": [
                    "cp",
                    "${build_dir}/${executable}",
                    "${dest_dir}"
                ]
            },
            {
                "before": false,
                "args": [
                    "mv",
                    "${build_dir}/${executable}_build.log",
                    "${dest_dir}"
                ]
            }
        ],
        "run": [
            {
                "before": false,
                "args": [
                    "grep",
                    "-h",
                    "\"Time in\"",
                    "run_${executable}*.log"
                ],
                "output": "time_${executable}.log"
            },
            {
                "before": false,
                "args": [
                    "grep",
                    "-h",
                    "\"Time in\"",
                    "run_${executable}*.log"
                ],
                "output": "time_${executable}.log"
            },
            {
                "before": false,
                "args": [
                    "rm",
                    "-rf",
                    "run_${executable}*.log"
                ],
                "output": ""
            }
        ]
    }
}
