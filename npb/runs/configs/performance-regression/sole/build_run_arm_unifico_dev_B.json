{
    "info": {},
    "tags": [
        "exec_time"
    ],
    "executables": [
        "bt_aarch64_aligned.out",
        "cg_aarch64_aligned.out",
        "ep_aarch64_aligned.out",
        "ft_aarch64_aligned.out",
        "is_aarch64_aligned.out",
        "lu_aarch64_aligned.out",
        "mg_aarch64_aligned.out",
        "sp_aarch64_aligned.out",
        "ua_aarch64_aligned.out"
    ],
    "env": {},
    "bin_dir": "bins",

    "*": {
        "npb_class": "B",
        "iterations": 3,
        "output": "run_${executable}_${iteration}.log",
        "prepend": [],
        "append": [],
        "build": [
            {
                "before": false,
                "args": [
                    "sshpass",
                    "-f",
                    "/home/nikos/phd/unified_abi/npb/runs/file.txt",
                    "scp",
                    "-r",
                    "nikos@nettuno:/home/nikos/phd/unified_abi/npb/runs/experiment_20230619_210656/bins/${executable}",
                    "${dest_dir}"
                ]
            },
            {
                "before": false,
                "args": [
                    "sshpass",
                    "-f",
                    "/home/nikos/phd/unified_abi/npb/runs/file.txt",
                    "scp",
                    "-r",
                    "nikos@nettuno:/home/nikos/phd/unified_abi/npb/runs/experiment_20230619_210656/bins/${executable}_build.log",
                    "${dest_dir}"
                ]
            }
        ],
        "run": [
            {
                "before": false,
                "args": [
                    "grep",
                    "-h",
                    "\"Time in\"",
                    "run_${executable}*.log"
                ],
                "output": "time_${executable}.log"
            },
            {
                "before": false,
                "args": [
                    "grep",
                    "-h",
                    "\"Time in\"",
                    "run_${executable}*.log"
                ],
                "output": "time_${executable}.log"
            },
            {
                "before": false,
                "args": [
                    "awk",
                    "'BEGIN{E = exp(1)} $2>0{tot+=log($2); c++} END{m=tot/c; printf \"%.2f\\n\", E^m}'",
                    "time_${executable}.log"
                ],
                "output": "geomean.log"
            },
            {
                "before": false,
                "args": [
                    "rm",
                    "-rf",
                    "run_${executable}*.log"
                ],
                "output": ""
            }
        ]
    }
}
