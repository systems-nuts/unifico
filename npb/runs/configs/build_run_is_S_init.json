{
    "info": {},
    "tags": [
        "exec_time"
    ],
    "executables": [
        "is_x86_64_init.out"
    ],
    "env": {},
    "bin_dir": "bins",

    "*": {
        "npb_class": "A",
        "iterations": 2,
        "output": "run_${executable}_${iteration}.log",
        "prepend": [],
        "append": [],
        "build": [
            {
                "before": true,
                "args": [
                    "make",
                    "clean"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "${npb_class}"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "-C",
                    "${build_dir}",
                    "-j10",
                    "${executable}",
                    "CFLAGS=-O0"
                ],
                "output": "${build_dir}/${executable}_build.log"
            },
            {
                "before": false,
                "args": [
                    "cp",
                    "${build_dir}/${executable}",
                    "${dest_dir}"
                ]
            },
            {
                "before": false,
                "args": [
                    "mv",
                    "${build_dir}/${executable}_build.log",
                    "${dest_dir}"
                ]
            }
        ],
        "commands": [
            {
                "before": false,
                "args": [
                    "grep",
                    "-h",
                    "\"Time in\"",
                    "run_${executable}*.log"
                ],
                "output": "time_${executable}.log"
            },
            {
                "before": false,
                "args": [
                    "grep",
                    "-h",
                    "\"Time in\"",
                    "run_${executable}*.log"
                ],
                "output": "time_${executable}.log"
            },
            {
                "before": false,
                "args": [
                    "awk",
                    "'BEGIN{E = exp(1)} $2>0{tot+=log($2); c++} END{m=tot/c; printf \"%.2f\\n\", E^m}'",
                    "time_${executable}.log"
                ],
                "output": "geomean.log"
            },
            {
                "before": false,
                "args": [
                    "rm",
                    "-rf",
                    "run_${executable}*.log"
                ],
                "output": ""
            }
        ]
    }
}
