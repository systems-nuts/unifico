{
    "info": {},
    "tags": [
        "exec_time"
    ],
    "executables": [
        "ep_aarch64_aligned.out",
        "is_aarch64_aligned.out"
    ],
    "env": {},

    "*": {
        "npb_class": "S",
        "output": "run_${executable}_${iteration}.log",
        "prepend": [],
        "append": [],
        "build": [
            {
                "before": true,
                "args": [
                    "make",
                    "clean"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "${npb_class}"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "-C",
                    "${build_dir}",
                    "-j10",
                    "${executable}",
                    "EXPERIMENT_MODE=1",
                    "LLC_FLAGS=\"-disable-block-align --mc-relax-all\""
                ],
                "output": "${build_dir}/${executable}_build.log"
            },
            {
                "before": false,
                "args": [
                    "cp",
                    "${build_dir}/${executable}",
                    "${dest_dir}"
                ]
            },
            {
                "before": false,
                "args": [
                    "mv",
                    "${build_dir}/${executable}_build.log",
                    "${dest_dir}"
                ]
            }
        ],
        "run": []
    }
}
