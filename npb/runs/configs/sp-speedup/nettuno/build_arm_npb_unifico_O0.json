{
    "info": {},
    "tags": [
        "exec_time"
    ],
    "executables": [
        "sp_aarch64_aligned.out"
    ],
    "env": {},
    "bin_dir": "bins",

    "*": {
        "npb_class": "A",
        "output": "run_${executable}_${iteration}.log",
        "prepend": [],
        "append": [],
        "build": [
            {
                "before": true,
                "args": [
                    "make",
                    "clean"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "${npb_class}"
                ]
            },
            {
                "before": true,
                "args": [
                    "make",
                    "-C",
                    "${build_dir}",
                    "-j10",
                    "${executable}"
                ],
                "output": "${build_dir}/${executable}_build.log"
            },
            {
                "before": false,
                "args": [
                    "cp",
                    "${build_dir}/${executable}",
                    "${dest_dir}"
                ]
            },
            {
                "before": false,
                "args": [
                    "mv",
                    "${build_dir}/${executable}_build.log",
                    "${dest_dir}"
                ]
            }
        ],
        "run": []
    }
}
